{
  "load_extensions": {
    "vim_binding/vim_binding": true
  },
  "Cell": {
    "cm_config": {
      "lineNumbers": true
    }
  },
  "Notebook": {
    "Toolbar": false,
    "Header": false
  },
  "keys": {
    "command": {
      "bind": {
        "cmd-space": "vim-binding:show-keyboard-shortcuts",
        "cmd-?": "vim-binding:show-keyboard-shortcuts"
      }
    }
  }
}